<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="MySQL" targetRuntime="MyBatis3">
        <plugin type="space.dfxyz.mybatis.generator.Plugin"/>
        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost/test"
                        userId="root" password="root"/>
        <javaModelGenerator targetPackage="output.models"
                            targetProject="${mybatis.generator.javaOutputDirectory}">
            <property name="constructorBased" value="true"/>
        </javaModelGenerator>
        <sqlMapGenerator targetPackage="output.mappers"
                         targetProject="${mybatis.generator.xmlOutputDirectory}"/>
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="output.mappers"
                             targetProject="${mybatis.generator.javaOutputDirectory}"/>
        <table tableName="test1"/>
        <table tableName="test2"/>
    </context>
</generatorConfiguration>
